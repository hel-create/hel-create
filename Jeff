import pandas as pd
import matplotlib.pyplot as plt

# 假设 avg_median_values 已经包含你需要的数据

# 设置图形的大小
plt.figure(figsize=(14, 8))

# 创建一个子图，用于 avg_gross_rental_income
plt.subplot(2, 1, 1)
plt.bar(avg_median_values['condo_counts'], avg_median_values['avg_Gross Rental Income'], color='b', alpha=0.7)
plt.xlabel('Condo Counts')
plt.ylabel('Average Gross Rental Income')
plt.title('Condo Counts vs Average Gross Rental Income')

# 创建另一个子图，用于 avg_net_rental_income
plt.subplot(2, 1, 2)
plt.bar(avg_median_values['condo_counts'], avg_median_values['avg_Net Rental Income'], color='g', alpha=0.7)
plt.xlabel('Condo Counts')
plt.ylabel('Average Net Rental Income')
plt.title('Condo Counts vs Average Net Rental Income')

# 调整布局以避免重叠
plt.tight_layout()
plt.show()
